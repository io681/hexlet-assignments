
@import exercise.dto.posts.PostPage
@import exercise.util.NamedRoutes
@param PostPage page

@template.layout.page(content = @`

    <main>
        <h1>${page.getPost().getName()}</h1>
        <p>${page.getPost().getBody()}</p>
    </main>
    <a href="${NamedRoutes.postsPath()}">Back to posts</a>
`)